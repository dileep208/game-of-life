// 
pipeline {
    agent any
    
        tools {
      maven 'maven'
    }
    
    stages {
        stage('Scm') {
            steps {
                git 'https://github.com/dileep208/game-of-life.git'
            }
        }
        stage('Build') {
            steps {

                sh 'mvn package'
            }
        }
    }
    post { 
        always { 
            archiveArtifacts  '**/*.war'
            junit '**/TEST-*.xml'
        }
    }
}

